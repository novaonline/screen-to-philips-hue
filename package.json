{
  "name": "screen-to-philips-hue",
  "version": "1.0.0",
  "description": "grabs the average color on screen and send the color to a philips hue",
  "main": "src/index.js",
  "dependencies": {
    "color-convert": "^1.9.0",
    "node-rest-client": "^3.1.0",
    "node-rest-client-promise": "^2.0.0",
    "robotjs": "^0.4.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1"
  },
  "scripts": {
